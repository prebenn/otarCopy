{
    "name": "otarCopy",
    "version": "0.2",
    "author": "Preben Nilsen",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://looker.wolt.com/dashboards/*"],
        "js": ["content.js"]
      }
    ],
    "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
     "128": "icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
      "default_title": "otarCopy"
    }
  }